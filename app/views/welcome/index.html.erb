      ----------------------------------------------- -->
      
      <div class="row work">

         <div class="three columns header-col">
            <h1><span>Work</span></h1>
         </div>

         <div class="nine columns main-col">
         <% @works.each do |work| %>
            <div class="row item">

               <div class="twelve columns">

                  <h3><%= work.title %></h3>
                  <p class="info"><%= work.role %><span>&bull;</span> <em class="date"><%= work.date %></em></p>

