            <p><%= about.about %>
            </p>

            <div class="row">

               <div class="columns contact-details">

                  <h2>Contact Details</h2>
                  <p class="address">
                     <span><%= about.name %></span><br>
                     <span><%= about.address %>
                     </span><br>
                     <span><%= about.phone %></span><br>
                     <span><%= about.email %></span>
                  </p>

               </div>

               <div class="columns download">
                  <p>
                     <a href="<%= about.resume_url %>" class="button"><i class="fa fa-download"></i>Download Resume </a>
                  </p>
               </div>

            </div> <!-- end row -->

         </div> <!-- end .main-col -->

      </div>

   </section> <!-- About Section End-->
<% end %>

   <!-- Resume Section
   ================================================== -->
